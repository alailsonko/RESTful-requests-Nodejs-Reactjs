{
    "extends": ["plugin:react/recommended", "prettier"],
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": 0
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }